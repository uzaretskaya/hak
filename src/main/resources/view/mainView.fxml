<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="ru.hackathon.controller.MainViewController">
    <VBox AnchorPane.leftAnchor="0.0">
        <Button fx:id="btChoise" onAction="#methodChoise" text="choise">
            <VBox.margin>
                <Insets bottom="3.0" left="3.0" right="3.0" top="3.0"/>
            </VBox.margin>
        </Button>
        <TableView fx:id="tableCameras" maxHeight="250" AnchorPane.bottomAnchor="0.0">
            <columns>
                <TableColumn fx:id="cameraIDColumn" prefWidth="100.0" text="Camera_id"/>
                <TableColumn fx:id="videoPathColumn" prefWidth="550.0" text="Video path"/>
            </columns>
            <VBox.margin>
                <Insets left="3.0"/>
            </VBox.margin>
        </TableView>
    </VBox>
</AnchorPane>
